{
    "name": "eimanavicius/wp-pjax",
    "type": "wordpress-plugin",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Peter Elmered",
            "email": "peter@elmered.com",
            "homepage": "http://www.elmered.com"
        },
        {
            "name": "Nerijus Eimanavičius",
            "email": "nerijus@eimanavicius.lt",
            "homepage": "http://nerijus.eimanavicius.lt"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "autoload": {
        "classmap": [
            "inc/",
            "views/settingsmenu.php"
        ],
        "files": [
            "inc/define.php"
        ]
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "wordpress/phpunit",
                "version": "master",
                "source": {
                    "type": "svn",
                    "url": "https://develop.svn.wordpress.org/trunk/tests/phpunit/",
                    "reference": "includes"
                }
            }
        }
    ],
    "require-dev": {
        "johnpbloch/wordpress": "^4.4",
        "phpunit/phpunit": "^4.8",
        "wordpress/phpunit": "dev-master"
    },
    "extra": {
        "wordpress-install-dir": "vendor/johnpbloch/wordpress"
    },
    "scripts": {
        "test": "phpunit"
    }
}
